<Window x:Class="somerpg_main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:somerpg_main"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen" WindowState="Maximized">
    <Grid>
        <ScrollViewer Margin="0" PanningMode="Both" HorizontalScrollBarVisibility="Visible" CanContentScroll="True">
            <Canvas x:Name="canvas" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" MouseDown="Window_MouseDown"/>
        </ScrollViewer>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="#FFAC0505" Width="200" Height="395" Panel.ZIndex="120"/>

    </Grid>
</Window>
